pipeline {
    agent any

    stages {
        stage('Test') {
            steps {
                sh 'chmod +x ./cosmos_django/jenkins_scripts/test.sh'
                sh './cosmos_django/jenkins_scripts/test.sh'
            }
        }
        stage('Deploy') {
            steps {
                sh 'chmod +x ./cosmos_django/jenkins_scripts/stage.sh'
                sh './cosmos_django/jenkins_scripts/stage.sh'
            }
        }
    }
}

